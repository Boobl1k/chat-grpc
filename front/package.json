{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run proto-build | react-scripts start",
    "build": "npm run proto-build | react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proto-build": "npm run proto-build-auth | npm run proto-build-chat",
    "proto-build-auth": "mkdir src\\proto\\generated\\auth | protoc --proto_path=./src/proto --js_out=import_style=commonjs,binary:./src/proto/generated/auth --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/proto/generated/auth auth.proto",
    "proto-build-chat": "mkdir src\\proto\\generated\\chat | protoc --proto_path=./src/proto --js_out=import_style=commonjs,binary:./src/proto/generated/chat --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/proto/generated/chat chat.proto"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
